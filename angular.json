{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "cp": {
      "root": "",
      "sourceRoot": "client",
      "projectType": "application",
      "prefix": "cp",
      "schematics": {
        "@schematics/angular:component": {
          "styleext": "scss"
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "_dist",
            "index": "client/index.html",
            "main": "client/main.ts",
            "showCircularDependencies": false,
            "polyfills": "client/polyfills.ts",
            "tsConfig": "client/tsconfig.app.json",
            "assets": ["client/favicon.ico", "client/public"],
            "styles": [
              "node_modules/@angular/cdk/overlay-prebuilt.css",
              "node_modules/pickr-widget/dist/pickr.min.css",
              "node_modules/bootstrap/scss/bootstrap.scss",
              "node_modules/quill/dist/quill.core.css",
              "node_modules/quill/dist/quill.snow.css",
              "node_modules/croppie/croppie.css",
              "node_modules/flatpickr/dist/flatpickr.css",
              "client/style/app.scss"
            ],
            "stylePreprocessorOptions": {
              "includePaths": ["client/style"]
            },
            "scripts": [
              "node_modules/jquery/dist/jquery.min.js",
              "node_modules/flatpickr/dist/flatpickr.min.js",
              "node_modules/bootstrap/dist/js/bootstrap.bundle.min.js"
            ]
          },
          "configurations": {
            "production_usa": {
              "fileReplacements": [
                {
                  "replace": "client/environments/environment.ts",
                  "with": "client/environments/environment.prod.usa.ts"
                },
                {
                  "replace": "client/index.html",
                  "with": "client/index.prod.html"
                }
              ],
              "optimization": false,
              "outputHashing": "all",
              "sourceMap": true,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            },
            "production_canada": {
              "fileReplacements": [
                {
                  "replace": "client/environments/environment.ts",
                  "with": "client/environments/environment.prod.canada.ts"
                },
                {
                  "replace": "client/index.html",
                  "with": "client/index.prod.html"
                }
              ],
              "verbose": true,
              "progress": false,
              "optimization": false,
              "outputHashing": "all",
              "sourceMap": true,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            },
            "staging": {
              "fileReplacements": [
                {
                  "replace": "client/environments/environment.ts",
                  "with": "client/environments/environment.staging.ts"
                },
                {
                  "replace": "client/index.html",
                  "with": "client/index.prod.html"
                }
              ],
              "optimization": false,
              "outputHashing": "all",
              "sourceMap": true,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "port": 3030,
            "browserTarget": "cp:build"
          },
          "configurations": {
            "staging": {
              "browserTarget": "cp:build:staging"
            },
            "production_usa": {
              "browserTarget": "cp:build:production_usa"
            },
            "production_canada": {
              "browserTarget": "cp:build:production_canada"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "cp:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "client/test.ts",
            "polyfills": "client/polyfills.ts",
            "tsConfig": "client/tsconfig.spec.json",
            "karmaConfig": "client/karma.conf.js",
            "styles": ["client/style/app.scss"],
            "stylePreprocessorOptions": {
              "includePaths": ["client/style"]
            },
            "scripts": [
              "node_modules/jquery/dist/jquery.min.js",
              "node_modules/flatpickr/dist/flatpickr.min.js",
              "node_modules/bootstrap/dist/js/bootstrap.bundle.min.js"
            ],
            "assets": ["client/favicon.ico", "client/public"]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["client/tsconfig.app.json", "client/tsconfig.spec.json"],
            "exclude": ["**/node_modules/**"]
          }
        }
      }
    },
    "cp-e2e": {
      "root": "e2e/",
      "projectType": "application",
      "prefix": "",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "cp:serve"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "e2e/tsconfig.e2e.json",
            "exclude": ["**/node_modules/**"]
          }
        }
      }
    },
    "ui": {
      "root": "libraries/ui",
      "sourceRoot": "libraries/ui/src",
      "projectType": "library",
      "prefix": "ready-ui",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "libraries/ui/tsconfig.lib.json",
            "project": "libraries/ui/ng-package.json"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "libraries/ui/src/test.ts",
            "tsConfig": "libraries/ui/tsconfig.spec.json",
            "karmaConfig": "libraries/ui/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["libraries/ui/tsconfig.lib.json", "libraries/ui/tsconfig.spec.json"],
            "exclude": ["**/node_modules/**"]
          }
        }
      }
    }
  },
  "defaultProject": "cp",
  "schematics": {
    "@schematics/angular:component": {
      "styleext": "scss"
    }
  }
}
