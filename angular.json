{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "campus-cloud": {
      "root": "projects/campus-cloud",
      "sourceRoot": "projects/campus-cloud/src",
      "projectType": "application",
      "prefix": "cp",
      "schematics": {
        "@schematics/angular:component": {
          "styleext": "scss"
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "_dist",
            "index": "projects/campus-cloud/src/index.html",
            "main": "projects/campus-cloud/src/main.ts",
            "showCircularDependencies": true,
            "polyfills": "projects/campus-cloud/src/polyfills.ts",
            "tsConfig": "projects/campus-cloud/tsconfig.app.json",
            "assets": ["projects/campus-cloud/src/favicon.ico", "projects/campus-cloud/src/assets"],
            "styles": [
              "node_modules/@angular/cdk/overlay-prebuilt.css",
              "node_modules/pickr-widget/dist/pickr.min.css",
              "node_modules/bootstrap/scss/bootstrap.scss",
              "node_modules/quill/dist/quill.core.css",
              "node_modules/quill/dist/quill.snow.css",
              "node_modules/croppie/croppie.css",
              "node_modules/flatpickr/dist/flatpickr.css",
              "projects/campus-cloud/src/styles.scss"
            ],
            "stylePreprocessorOptions": {
              "includePaths": ["projects/campus-cloud/src/styles"]
            },
            "scripts": [
              "node_modules/jquery/dist/jquery.min.js",
              "node_modules/flatpickr/dist/flatpickr.min.js",
              "node_modules/bootstrap/dist/js/bootstrap.bundle.min.js"
            ]
          },
          "configurations": {
            "production_usa": {
              "fileReplacements": [
                {
                  "replace": "projects/campus-cloud/src/environments/environment.ts",
                  "with": "projects/campus-cloud/src/environments/environment.prod.usa.ts"
                },
                {
                  "replace": "projects/campus-cloud/src/index.html",
                  "with": "projects/campus-cloud/src/index.prod.html"
                }
              ],
              "statsJson": false,
              "deployUrl": "/dist/",
              "optimization": false,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": true,
              "buildOptimizer": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            },
            "production_canada": {
              "fileReplacements": [
                {
                  "replace": "projects/campus-cloud/src/environments/environment.ts",
                  "with": "projects/campus-cloud/src/environments/environment.prod.canada.ts"
                },
                {
                  "replace": "projects/campus-cloud/src/index.html",
                  "with": "projects/campus-cloud/src/index.prod.html"
                }
              ],
              "verbose": true,
              "progress": false,
              "deployUrl": "/dist/",
              "optimization": false,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": true,
              "buildOptimizer": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            },
            "staging": {
              "fileReplacements": [
                {
                  "replace": "projects/campus-cloud/src/environments/environment.ts",
                  "with": "projects/campus-cloud/src/environments/environment.staging.ts"
                },
                {
                  "replace": "projects/campus-cloud/src/index.html",
                  "with": "projects/campus-cloud/src/index.prod.html"
                }
              ],
              "statsJson": true,
              "deployUrl": "/dist/",
              "optimization": false,
              "outputHashing": "all",
              "sourceMap": true,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": true,
              "buildOptimizer": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "port": 3030,
            "browserTarget": "campus-cloud:build"
          },
          "configurations": {
            "staging": {
              "browserTarget": "campus-cloud:build:staging"
            },
            "production_usa": {
              "browserTarget": "campus-cloud:build:production_usa"
            },
            "production_canada": {
              "browserTarget": "campus-cloud:build:production_canada"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/campus-cloud/src/test.ts",
            "polyfills": "projects/campus-cloud/src/polyfills.ts",
            "tsConfig": "projects/campus-cloud/tsconfig.spec.json",
            "karmaConfig": "projects/campus-cloud/karma.conf.js",
            "styles": ["projects/campus-cloud/src/styles.scss"],
            "stylePreprocessorOptions": {
              "includePaths": ["projects/campus-cloud/src/styles"]
            },
            "scripts": [
              "node_modules/jquery/dist/jquery.min.js",
              "node_modules/flatpickr/dist/flatpickr.min.js",
              "node_modules/bootstrap/dist/js/bootstrap.bundle.min.js"
            ],
            "assets": ["projects/campus-cloud/src/favicon.ico", "projects/campus-cloud/src/assets"]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/campus-cloud/tsconfig.app.json",
              "projects/campus-cloud/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**"]
          }
        }
      }
    }
  },
  "defaultProject": "campus-cloud",
  "schematics": {
    "@schematics/angular:component": {
      "styleext": "scss"
    }
  }
}
