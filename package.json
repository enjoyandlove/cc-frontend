{
  "name": "oohlalamobile_control_panel",
  "version": "1.0.2",
  "description": "Control Panel",
  "engines": {
    "node": ">=8.2.1"
  },
  "scripts": {
    "i18n": "phraseapp pull",
    "clean": "rimraf node_modules doc dist && yarn run cache clean",
    "clean-install": "yarn run clean && yarn run install",
    "clean-start": "yarn run clean-install && yarn start",
    "watch": "webpack --watch --progress --profile",
    "production-usa": "webpack --progress --profile --bail && rimraf dist && mv _dist dist",
    "production-canada": "webpack --progress --profile --bail && rimraf dist && mv _dist dist",
    "production-sea": "webpack --progress --profile --bail && rimraf dist && mv _dist dist",
    "staging": "webpack --progress --profile --bail && rimraf dist && mv _dist dist",
    "build:prod-usa": "yarn run production-usa",
    "build:prod-can": "yarn run production-canada",
    "build:prod-sea": "yarn run production-sea",
    "build:staging": "yarn run staging",
    "server": "webpack-dev-server --inline --progress --port 3030",
    "webdriver-update": "webdriver-manager update",
    "webdriver-start": "webdriver-manager start",
    "lint": "tslint --force \"client/**/*.ts\"",
    "e2e": "protractor",
    "e2e-live": "protractor --elementExplorer",
    "pretest": "yarn run lint",
    "test": "karma start",
    "precommit": "pretty-quick --staged",
    "posttest": "remap-istanbul -i coverage/json/coverage-final.json -o coverage/html -t html",
    "test-watch": "karma start --no-single-run --auto-watch",
    "ci": "yarn run e2e && yarn run test",
    "docs": "typedoc --options typedoc.json client/app/app.component.ts",
    "start": "yarn run server",
    "start:hmr": "yarn run server -- --hot",
    "postinstall": "npm rebuild node-sass --force && yarn run webdriver-update"
  },
  "author": "andres@oohlalamobile.com",
  "license": "ISC",
  "dependencies": {
    "@angular/common": "5.0.0",
    "@angular/compiler": "5.0.0",
    "@angular/compiler-cli": "5.0.0",
    "@angular/core": "5.0.0",
    "@angular/forms": "5.0.0",
    "@angular/http": "5.0.0",
    "@angular/platform-browser": "5.0.0",
    "@angular/platform-browser-dynamic": "5.0.0",
    "@angular/platform-server": "5.0.0",
    "@angular/router": "5.0.0",
    "@ngrx/core": "^1.2.0",
    "@ngrx/store": "^4.1.0",
    "amplitude-js": "^3.8.0",
    "chartist": "^0.11.0",
    "chartist-plugin-tooltips": "0.0.17",
    "core-js": "^2.4.1",
    "croppie": "^2.4.1",
    "file-saver": "^1.3.3",
    "flatpickr": "2.4.4",
    "jquery": "^3.1.1",
    "jspdf": "^1.3.3",
    "lodash": "^4.17.4",
    "moment": "^2.17.1",
    "papaparse": "^4.3.6",
    "quill": "^1.3.2",
    "raven-js": "^3.17.0",
    "reflect-metadata": "^0.1.3",
