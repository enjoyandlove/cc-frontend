{
  "name": "oohlalamobile_control_panel",
  "version": "1.0.2",
  "description": "Control Panel",
  "engines": {
    "node": ">=8.9.4"
  },
  "scripts": {
    "i18n": "phraseapp pull",
    "production-usa":
      "node --max_old_space_size=8192 node_modules/.bin/ng build --c=production_usa --deployUrl=/dist/ && rimraf dist && mv _dist dist",
    "production-canada":
      "node --max_old_space_size=8192 node_modules/.bin/ng build --c=production_canada --deployUrl=/dist/ && rimraf dist && mv _dist dist",
    "staging":
      "node --max_old_space_size=8192 node_modules/.bin/ng build --c=staging --deployUrl=/dist/ && rimraf dist && mv _dist dist",
    "_posttest": "remap-istanbul -i coverage/json/coverage-final.json -o coverage/html -t html",
    "build:prod-usa": "yarn run production-usa",
    "build:prod-can": "yarn run production-canada",
    "build:staging": "yarn run staging",
    "e2e": "protractor",
    "e2e-live": "protractor --elementExplorer",
    "test": "node_modules/@angular/cli/bin/ng test --watch=false --codeCoverage --sourceMap=false",
    "test-watch": "node_modules/@angular/cli/bin/ng test",
    "ci": "yarn run e2e && yarn run test",
    "start": "ng serve",
    "precommit": "pretty-quick --staged"
  },
  "author": "andres@oohlalamobile.com",
  "license": "ISC",
